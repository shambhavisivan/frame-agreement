<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
<customSettingsType>Hierarchy</customSettingsType>
    <description>Frame Agreement Console settings (FAC settings) control the behaviour of the application. </description>
    <fields>
        <fullName>account_fields__c</fullName>
        <description>Additional fields to be shown in account table while associating them with Frame Agreement</description>
        <inlineHelpText>Name,csfam__Active__c,Type</inlineHelpText>
        <externalId>false</externalId>
        <label>Account Fields</label>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>active_status__c</fullName>
        <description>Used to define which Frame Agreement status value is used for activating Frame Agreement</description>
        <inlineHelpText>Active</inlineHelpText>
        <externalId>false</externalId>
        <label>Active Status</label>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <length>255</length>
    </fields>
    <fields>
        <fullName>approval_process_name__c</fullName>
        <description>Name of the approval process which will be invoked as a result of the negotiation</description>
        <inlineHelpText>FA Approval</inlineHelpText>
        <externalId>false</externalId>
        <label>Approval Process Name</label>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <length>255</length>
    </fields>
    <fields>
        <fullName>approved_status__c</fullName>
        <description>Once Frame Agreement is approved we will change the status to this value</description>
        <inlineHelpText>Approved</inlineHelpText>
        <externalId>false</externalId>
        <label>Approved Status</label>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <length>255</length>
    </fields>
    <fields>
        <fullName>approvers_revise__c</fullName>
        <description>Allow approver to edit FA while in "Pending" state.</description>
        <externalId>false</externalId>
        <label>Approvers revise</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
        <defaultValue>true</defaultValue>
    </fields>
    <fields>
        <fullName>closed_status__c</fullName>
        <description>Status of Frame Agreement once it becomes closed replaced. E.g. when a new version is activated.</description>
        <inlineHelpText>Closed Replaced</inlineHelpText>
        <externalId>false</externalId>
        <label>Closed Status</label>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <length>255</length>
    </fields>
    <fields>
        <fullName>decomposition_chunk_size__c</fullName>
        <description>Number of items we will process on decomposition in one remote action. Used to tweak performance in case of issues with Salesforce limits.</description>
        <externalId>false</externalId>
        <label>Decomposition Chunk Size</label>
        <precision>18</precision>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>discount_as_price__c</fullName>
        <description>If selected, negotiated discount will be shown as new price instead of price difference.</description>
        <externalId>false</externalId>
        <label>Discount As Price</label>
        <type>Checkbox</type>
        <defaultValue>true</defaultValue>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>draft_status__c</fullName>
        <description>Used to define which status will be used in Draft state.</description>
        <inlineHelpText>Draft</inlineHelpText>
        <externalId>false</externalId>
        <label>Draft Status</label>
        <type>Text</type>
        <unique>false</unique>
        <length>255</length>
    </fields>
    <fields>
        <fullName>fa_editable_statuses__c</fullName>
        <description>CSV list of statuses in which editing and negotiation can happen.</description>
        <inlineHelpText>Draft, Requires Approval</inlineHelpText>
        <externalId>false</externalId>
        <label>FA Editable Statuses</label>
        <type>TextArea</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>fa_number_prefix__c</fullName>
        <description>Prefix that is going to be assigned to Frame Agreement Number.</description>
        <inlineHelpText>FA_</inlineHelpText>
        <externalId>false</externalId>
        <label>FA Number Prefix</label>
        <type>Text</type>
        <unique>false</unique>
        <length>255</length>
    </fields>
    <fields>
        <fullName>frame_agreement_fields__c</fullName>
        <description>Additional Frame Agreement fields to show in FAM.</description>
        <inlineHelpText>csconta__agreement_level__c, csconta__Valid_From__c</inlineHelpText>
        <externalId>false</externalId>
        <label>Frame Agreement Fields</label>
        <type>TextArea</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>input_minmax_restriction__c</fullName>
        <description>When true prevents user to input value less than 0 and greater than charges original value.</description>
        <externalId>false</externalId>
        <label>Negotiate min/max restriction</label>
        <type>Checkbox</type>
        <unique>false</unique>
        <defaultValue>true</defaultValue>
    </fields>
    <fields>
        <fullName>price_item_fields__c</fullName>
        <description>Populate if additional fields should be displayed in Commercial Product lists.</description>
        <externalId>false</externalId>
        <label>Price Item Fields</label>
        <type>TextArea</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>product_chunk_size__c</fullName>
        <description>Size of products to be loaded in one remote action.</description>
        <externalId>false</externalId>
        <label>Product Chunk Size</label>
        <precision>18</precision>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>rcl_fields__c</fullName>
        <description>Additional Rate Card Line fields used for filtering.</description>
        <inlineHelpText>description__c</inlineHelpText>
        <externalId>false</externalId>
        <label>RCL Fields</label>
        <type>TextArea</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>requires_approval_status__c</fullName>
        <description>Used to define which Frame Agreement status will be used when approval is required.</description>
        <inlineHelpText>Requires Approval</inlineHelpText>
        <externalId>false</externalId>
        <label>Requires Approval Status</label>
        <type>Text</type>
        <length>255</length>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>new_frame_agreement__c</fullName>
        <description>Toggle visibility of "New Frame Agreement" button</description>
        <inlineHelpText>Requires Approval</inlineHelpText>
        <externalId>false</externalId>
        <label>Show New Frame Agreement</label>
        <type>Checkbox</type>
        <defaultValue>true</defaultValue>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>show_volume_fields__c</fullName>
        <description>If selected, additional volume and usage fields will be shown for each Commercial Product</description>
        <externalId>false</externalId>
        <label>Show Volume Fields</label>
        <type>Checkbox</type>
        <defaultValue>true</defaultValue>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>volume_fields_visibility__c</fullName>
        <description>Additional fields to be shown in account table while associating them with Frame Agreement. On the condition that csfam__show_volume_fields__c is set to true: here you can list volume fields you want to show. Fields are respresented with abbreviation (mv,mvp,muc,mucp)</description>
        <inlineHelpText>mv,mvp</inlineHelpText>
        <externalId>false</externalId>
        <label>Show Volume Fields</label>
        <type>TextArea</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>truncate_product_fields__c</fullName>
        <description>Used to create a nicer UI by truncating long values in custom fields.</description>
        <externalId>false</externalId>
        <label>Truncate CP Fields</label>
        <type>Checkbox</type>
        <defaultValue>true</defaultValue>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>usage_type_fields__c</fullName>
        <description>Additional Usage type fields.</description>
        <inlineHelpText>mv,mvp</inlineHelpText>
        <externalId>false</externalId>
        <label>Usage Type Fields</label>
        <type>TextArea</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>active_status_management__c</fullName>
        <description>On threshold breach while negotiating: FAM will automatically change status of FA to requires_approval_status__c. If all charges are within the threshold limits, it will return it back to draft_status__c.</description>
        <externalId>false</externalId>
        <label>Active Status Management</label>
        <type>Checkbox</type>
        <defaultValue>true</defaultValue>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>standalone_addon_fields__c</fullName>
        <description>CSV of fully qualified field names to be used when showing standalone addons</description>
        <externalId>false</externalId>
        <label>Standalone Addon Fields</label>
        <type>TextArea</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>decimal_places__c</fullName>
        <description>Fields containing price will be rounded to maximum this much decimals.</description>
        <externalId>false</externalId>
        <label>Decimal Places</label>
        <precision>18</precision>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>FAC Setting</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
</CustomObject>